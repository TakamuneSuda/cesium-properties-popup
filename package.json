{
	"name": "cesium-properties-popup",
	"version": "0.2.1",
	"description": "A Svelte component for displaying entity properties on hover or click in Cesium",
	"type": "module",
	"license": "MIT",
	"keywords": [
		"svelte",
		"cesium",
		"globe"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/takamunesuda/cesium-properties-popup.git"
	},
	"author": "takamunesuda (https://x.com/STakamu2532)",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-package",
		"build:demo": "vite build",
		"package": "svelte-kit sync && svelte-package && publint",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prepublishOnly": "pnpm run check && pnpm run package"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.3.11",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"cesium": "^1.129.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.2.12",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.5.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"peerDependencies": {
		"cesium": "^1.100.0",
		"svelte": "^5.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js"
}